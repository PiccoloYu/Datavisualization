[
    // ctrl+shift+/多行注释
    {
    "key": "ctrl+shift+/",
    "command": "editor.action.blockComment",
    "when": "editorTextFocus"
    },
    // ctrl+d 删除一行
    {
    "key": "ctrl+d",
    "command": "editor.action.deleteLines",
    "when": "editorTextFocus"
    },
    // 快速添加注释信息
    {
    "key": "ctrl+alt+j",
    // "command": "docthis.documentEverythingVisible"
    // "command": "docthis.documentEverything"
    "command": "docthis.documentThis"
    },
]